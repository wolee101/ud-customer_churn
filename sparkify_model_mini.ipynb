    "get_song = udf(lambda x: 1 if pd.isnull(x) == False else 0, IntegerType())\n",
    "df = df.withColumn('song_played', get_song(df.song)).withColumn('num_songs', Fsum('song_played').over(w))\n",
    "df = df.withColumn('avg_num_songs', (df.num_songs/df.num_sessions))"
